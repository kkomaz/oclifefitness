<!-- preloader -->
<div class="preloading">
  <%= link_to image_tag("fancy-loading.gif", class: "loading-gif"), root_path %>
</div>
<!--################ HEADER START ################-->
  <header class="page-title">
    <div class="container">
      <h2>Contact</h2>
    </div>
  </header>

  <section class="contact-page">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <h1 class="center">Get in Touch</h1>
          <p class="description center in-touch">We are always happy to hear from you so if you have any questions, comments, or inquiries please feel free to contact us at any time.</p>
          <p class="description center in-touch">We are available to serve your needs 24 hours/day 7 days a week.</p>
          <br>
          <div class="map">
            <iframe height="600" src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3319.653995322716!2d-117.85444869999999!3d33.6920224!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x80dcdeeaac871103%3A0x78eb6715596ce6b0!2sOC+Life+Fitness!5e0!3m2!1sen!2sus!4v1439585680963"></iframe>
          </div>
          <br>
        </div>
    </div>
  </section>

  <section class="contact-page rev">
    <div class="container">
      <div class="row margintop20">
        <div class="col-md-7">
          <h2 class="big-h2-heading"><i class="oi oi-envelope-open"></i> Contact Us</h2>
          <div class="contact-description">
            <p>We understand that some people prefer one method of communication over another. That's why we make it easy to get in touch with us. </p>
            <p>Our training facilities are located throughout South Orange County with flagship gyms conveniently located in Irvine and Newport Beach. </p>
          </div>
          <br>
          <%= render 'form' %>
          <div id="result"></div>
        </div>
        <div class="col-md-4 col-md-offset-1">
            <h1>Address</h1>
            <h4><i class="icon-road"></i> 17811 Sky Park Cir, Irvine, CA 92614</h4>
            <h4><i class="icon-phone"></i> 949.335.8680 </h4>
            <h4><i class="icon-envelope"></i>  <a href="mailto:info@oclifefitness.com">info@oclifefitness.com</a></h4>
            <h2>Social Media</h2>
            <a href="https://www.facebook.com/pages/OCLifeFitness/100850386621246?fref=ts" class="social-network sn2 facebook" target="_blank"></a>
            <a href="http://www.yelp.com/biz/oc-life-fitness-irvine-2" class="social-network sn2 yelp" target="_blank"></a>
        </div>
      </div>
    </div>
  </section>


</div>

